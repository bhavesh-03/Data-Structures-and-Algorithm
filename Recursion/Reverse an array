// Two variable
#include <bits/stdc++.h>
using namespace std ;
 

// reverse a array
void reverse(int l,int r,int a[]){
   if(l>=r){
        return ;  
   }
   swap(a[l],a[r]);
   reverse(l+1,r-1,a);
}
 
int main(){
  int n=8;
  int a[n]={1,2,3,4,6,7,8,9};
  reverse(0,n-1,a);
  for(int i=0;i<n;i++){
    cout<<a[i]<<" ";
  }
  cout<<endl;

}

