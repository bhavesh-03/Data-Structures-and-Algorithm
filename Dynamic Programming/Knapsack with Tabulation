
#include <bits/stdc++.h>
using namespace std ;


int main(){
  int n ;
  cin>>n;
  std::vector<int> wt(n);
  std::vector<int> profit(n);
  for(int i=0;i<n;i++){
    cin>>wt[i];
  }
  for(int i=0;i<n;i++){
    cin>>profit[i];
  }
  int maxwt ;
  cin>>maxwt;

  int dp[n+1][maxwt+1];

    for (int i = 0; i < n+1; i++)
        dp[i][0] = 0;
    for (int i = 0; i < maxwt+1; i++)
        dp[0][i] = 0;

    for (int i = 1; i < n+1; i++){
        for (int j = 1; j < maxwt+1; j++){
            if (wt[i-1] <= j){
                dp[i][j] = max(wt[i-1]+dp[i-1][j-wt[i-1]], dp[i-1][j]);
            }else
                dp[i][j] = dp[i-1][j];
         }
    }
    cout<< dp[n][maxwt]<<endl;

}

